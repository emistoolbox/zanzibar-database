{
    "lookups": {
 		"EmisIdLookup": { 
            "type": "file", 
            "keyColumn": "interview__id", 
            "valueColumn": "emis_id", 
            "source": {
                "type": "union", 
                "headers": [ "interview__id", "emis_id" ], 
                "sources": [
					{ "type": "csv", "filename": "${DATA}/2018/PRE-PRIMARY/Zanzibar MOEVT PRE-PRIMARY ASC.tab" },
                    { "type": "csv", "filename": "${DATA}/2018/PRIMARY/Zanzibar MOEVT PRIMARY ASC.tab" },
                    { "type": "csv", "filename": "${DATA}/2018/SECONDARY/Zanzibar MOEVT SECONDARY ASC.tab" }
                ]
            }
        },  
 		"EducationLevelLookup": { 
            "type": "file", 
            "keyColumn": "interview__id", 
            "valueColumn": "education_level_id", 
            "source": {
                "type": "union", 
                "headers": [ "interview__id", "education_level_id" ], 
                "sources": [
					{ "type": "csv", "filename": "${DATA}/2018/PRE-PRIMARY/Zanzibar MOEVT PRE-PRIMARY ASC.tab" },
                    { "type": "csv", "filename": "${DATA}/2018/PRIMARY/Zanzibar MOEVT PRIMARY ASC.tab" },
                    { "type": "csv", "filename": "${DATA}/2018/SECONDARY/Zanzibar MOEVT SECONDARY ASC.tab" }
                ]
            }
        }, 
 		"YearLookup": { 
            "type": "file", 
            "keyColumn": "interview__id", 
            "valueColumn": "year", 
            "source": {
                "type": "union", 
                "headers": [ "interview__id", "year" ], 
                "sources": [
					{ "type": "csv", "filename": "${DATA}/2018/PRE-PRIMARY/Zanzibar MOEVT PRE-PRIMARY ASC.tab" },
                    { "type": "csv", "filename": "${DATA}/2018/PRIMARY/Zanzibar MOEVT PRIMARY ASC.tab" },
                    { "type": "csv", "filename": "${DATA}/2018/SECONDARY/Zanzibar MOEVT SECONDARY ASC.tab" }
                ]
            }
        }
    },
    
	"processes": [
		{
			"source": { 
				"type": "union", 
				"headers": [ "emis_id", "year", "education_level_id", "leave_id", "education_id", "gender_id", "qty" ],
				"sources": [
					{ "type": "csv", "filename": "${DATA}/2018/PRE-PRIMARY/r_teachers_leave_qty.tab" },
                    { "type": "csv", "filename": "${DATA}/2018/PRIMARY/r_teachers_leave_qty.tab" },
                    { "type": "csv", "filename": "${DATA}/2018/SECONDARY/r_teachers_leave_qty.tab" }
				],
				"filters": [
                    { "type": "rename", "renames": { "r_teachers_leave__id": "leave_id", "r_teachers_leave_qty__id": "education_id" }},
					{ "type": "copy", "copies": { "year": "interview__id", "emis_id": "interview__id", "education_level_id": "interview__id" } }, 
					{ "type": "map", "mappings": { "year": "YearLookup", "emis_id": "EmisIdLookup", "education_level_id": "EducationLevelLookup" }},
					{
						"type": "expand", 
						"valueColumn": "qty",
						"copyColumns": [ "emis_id", "year", "education_level_id", "leave_id", "education_id" ],
						"constColumnHeaders": [ "gender_id" ],
						"constColumnValues" : {
							"male_leave_qty": "0",
							"female_leave_qty": "1"
						}
					}
				]
			},
			"pipes": [
				{
					"target": { 
						"type": "sql", 
						"table": "teacher_leaves", 
						"deleteWhere": "year = 2018", 
						"notNullColumns": [ "emis_id", "year", "education_level_id" ], 
						"columns": { 
							"emis_id": "emis_id",
							"year": "year",
                            "education_level_id": "education_level_id",
							"gender_id": "gender_id",
                            "teacher_edu_condensed_id": "education_id",
                            "teacher_leave_id": "leave_id",
                            "qty": "qty"
						}
					}
				}
			]
		}
	]
}
